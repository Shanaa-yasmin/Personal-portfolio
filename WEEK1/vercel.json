{
  "version": 2,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "node build.js",
  "outputDirectory": "dist"
}
